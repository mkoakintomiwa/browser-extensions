{
    "name": "Auto Fill Fields",
    "version": "1.0",
    "description": "Help fill fields programmatically",
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
    ],
    "permissions": [
        "tabs"
    ],
    "manifest_version": 2,
    "icons": {
        "16": "images/icons/16.png",
        "32": "images/icons/32.png",
        "48": "images/icons/48.png",
        "128": "images/icons/128.png"
      }
  }